{
  "name": "Bomb! Bot",
  "description": "Bot nhạc dành riêng cho Eden's Land. Code bởi Sena dựa trên Discord-Musicbot",
  "repository": "https://github.com/SudhanPlayz/Discord-MusicBot",
  "logo": "https://cdn.discordapp.com/avatars/750613142488481843/e6326038dbe2243ca551ba5b6ecd8bf2.png?size=1024",
  "keywords": [
    "node",
    "discord",
    "youtube",
    "music",
    "bot",
    "lavalink",
    "dashboard"
  ],
  "env": {
    "Prefix": {
      "description": "Tiền tố lệnh",
      "required": "true"
    },
    "Token": {
      "description": "Bot Token",
      "required": "true"
    },
    "Discord_ClientID": {
      "description": "ClientID",
      "required": "true"
    },
    "Discord_ClientSecret": {
      "description": "Client Secret",
      "required": "true"
    },
    "Website": {
      "description": "URL của Webserver (Ví dụ: https://domain.xyz).",
      "value": "https://bot.bomb-bot.ga"
    },
    "Spotify_ClientID": {
      "description": "Spotify ClientID.",
      "required": "true"
    },
    "Spotify_ClientSecret": {
      "description": "Spotify ClientSecret.",
      "required": "true"
    }
  }
}
